	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#676773"
			}
		},
		{
			"name": "Keyword, Storage, Escape sequence, language constants, delete",
			"scope": [
				"keyword",
				"keyword.operator.instanceof",
				"variable.language",
				"storage",
                "constant.character",
                "constant.language",
                "keyword.operator.delete"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#62a362"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
				"constant.other.color"
			],
			"settings": {
				"foreground": "#4dacf0"
			}
		},
		{
			"name": "Variable inside class",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#93a6f5"
			}
		},
		{
			"name": "Function declaration",
			"scope": [
				"entity.name.function.member",
				"entity.name.function.definition",
				"entity.name.function.python",
				"meta.method.identifier.java",
			],
			"settings": {
				"foreground": "#c7a65d"
			}
		},
		{
			"name": "Properties (aka: keys)",
			"scope": [
				"meta.property-name",
				"entity.other.attribute-name.html",
				"entity.other.attribute-name.namespace.xml",
				"entity.other.attribute-name.localname.xml",
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "C++: directives, NULL",
			"scope": [
				"keyword.control.directive",
				"entity.name.function.preprocessor",
                "constant.language.NULL"
			],
			"settings": {
				"foreground": "#85a658"
			}
		},
		{
			"name": "C++: character",
			"scope": [
				"string.quoted.single.cpp"
			],
			"settings": {
				"foreground": "#4dacf0"
			}
		},
		{
			"name": "C++: Function call",
			"scope": [
				"entity.name.function.call"
			],
			"settings": {
				"foreground": "#c7a65d"
			}
		},
		{
			"name": "C++: defaults",
			"scope": [
                "entity.name.function.definition.special.constructor",
				"punctuation.separator",
				"keyword.operator",
			],
			"settings": {
				"foreground": "#c9c9d1"
			}
		},
		{
			"name": "HTML: doctype",
			"scope": [
				"meta.tag.metadata.doctype"
			],
			"settings": {
				"foreground": "#85a658"
			}
		},
		{
			"name": "HTML: tag name",
			"scope": [
				"meta.tag.metadata",
				"meta.tag.structure"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "XML: preprocessor",
			"scope": [
				"meta.tag.preprocessor.xml"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "XML: tag",
			"scope": [
				"entity.name.tag.localname",
				"punctuation.definition.tag.xml"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector.css"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "CSS: Value",
			"scope": [
				"meta.property-value"
			],
			"settings": {
				"foreground": "#62a362"
			}
		},
		{
			"name": "CSS: Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#62a362"
			}
		},
		{
			"name": "CSS: functions",
			"scope": [
				"meta.function.shape.css",
				"meta.function.misc.css"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "CSS: @media",
			"scope": [
				"keyword.control.at-rule"
			],
			"settings": {
				"foreground": "#93a6f5"
			}
		},
		{
			"name": "CSS: @media header",
			"scope": [
				"meta.at-rule",
				"keyword.operator.logical.and.media",
				"keyword.operator.logical.only.media"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "CSS: defaults",
			"scope": [
				"entity.name.tag.wildcard",
			],
			"settings": {
				"foreground": "#c9c9d1"
			}
		},
		{
			"name": "JAVA: Annotation",
			"scope": [
				"storage.type.annotation",
				"punctuation.definition.annotation"
			],
			"settings": {
				"foreground": "#85A658"
			}
		},
		{
			"name": "JAVA: non-primitive data types",
			"scope": [
				"storage.type.java",
				"storage.type.generic.java"
			],
			"settings": {
				"foreground": "#37cccc"
			}
		},
		{
			"name": "JAVA: variable inside class (not correct!)",
			"scope": [
				"variable.other.definition.java"
			],
			"settings": {
				"foreground": "#93a6f5"
			}
		},
		{
			"name": "JAVA: defaults",
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
				"variable.other.object",
				"variable.parameter.java"
			],
			"settings": {
				"foreground": "#c9c9d1"
			}
		},
		{
			"name": "JS: variable",
			"scope": [
				"meta.definition.variable.js"
			],
			"settings": {
				"foreground": "#93a6f5"
			}
		},
		{
			"name": "JS: function name",
			"scope": [
				"entity.name.function.js"
			],
			"settings": {
				"foreground": "#ffc66d"
			}
		},
		{
			"name": "JSON: property",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "Python: defaults",
			"scope": [
				"variable.language.special.self.python"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#c9c9d1"
			}
		},
		{
			"name": "Markdown: Headings, separator",
			"scope": [
				"heading",
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "#93a6f5"
			}
		},
		{
			"name": "Markdown: operators",
			"scope": [
				"punctuation.definition.list.begin.markdown",
				"meta.separator.markdown",
				"meta.image.inline.markdown",
				"string.other.link.description.markdown",
				"punctuation.definition.bold.markdown"
			],
			"settings": {
				"foreground": "#e0957b"
			}
		},
		{
			"name": "Markdown: path",
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#50afde"
			}
		},
		{
			"name": "Markdown: bold",
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		
	]
}
