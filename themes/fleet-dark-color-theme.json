				"storage.modifier.import",
				"variable.other.object"
			],
			"settings": {
				"foreground": "#d1d1d1"
			}
		},
		{
			"name": "JS: variable",
			"scope": [
				"meta.definition.variable"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "JS: function name",
			"scope": [
				"entity.name.function.js"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "JSON: property",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "Python: Function call / Same in java",
			"scope": [
				"meta.function-call.python"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "Python: defaults",
			"scope": [
				"meta.function-call.arguments",
				"variable.language.special.self.python"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d1d1d1"
			}
		},
		{
			"name": "Markdown: Headings",
			"scope": [
				"heading"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "Markdown: operators",
			"scope": [
				"punctuation.definition.list.begin.markdown",
