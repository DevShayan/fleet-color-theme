{
	"name": "Fleet Color Theme",
	"colors": {
		"editor.background": "#181818",
		"editor.foreground": "#d1d1d1",
		"editor.lineHighlightBackground": "#292929",
		"editor.lineHighlightBorder": "#00000000",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#181818",
		"sideBar.border": "#383838",
		"tab.inactiveBackground": "#292929",
		"tab.activeBackground": "#181818",
		"tab.activeBorderTop": "#d1d1d1",
		"editorLineNumber.foreground": "#5d5d5d",
		"editorLineNumber.activeForeground": "#c2c2c2",
		"editorLineNumber.dimmedForeground": "#5d5d5d",
		"editorIndentGuide.background": "#00000000",
		"editorIndentGuide.activeBackground": "#00000000",
		"editorBracketMatch.background": "#163764",
		"editorBracketMatch.border": "#00000000",
		"editorBracketHighlight.foreground1": "#d1d1d1",
		"editorBracketHighlight.foreground2": "#d1d1d1",
		"editorBracketHighlight.foreground3": "#d1d1d1",
		"editorBracketHighlight.foreground4": "#d1d1d1",
		"editorBracketHighlight.foreground5": "#d1d1d1",
		"editorBracketHighlight.foreground6": "#d1d1d1",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#898989"
			}
		},
		{
			"name": "Keyword and Storage",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#e394dc"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "Escape Sequence",
			"scope": [
				"constant.character"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "C++: directives",
			"scope": [
				"keyword.control.directive",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "C++: bools",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "C++: character",
			"scope": [
				"string.quoted.single.cpp"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "C++: namespace name",
			"scope": [
				"entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "C++: constructor",
			"scope": [
				"entity.name.function.definition.special.constructor"
			],
			"settings": {
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "C++: function name",
			"scope": [
				"entity.name.function.definition",
				"entity.name.function.call"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#efefef"
			}
		},
		{
			"name": "C++: class and struct name",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "C++: NULL",
			"scope": [
				"constant.language.NULL"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "C++: defaults",
			"scope": [
				"punctuation.separator",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#d1d1d1"
			}
		},
		{
			"name": "C++: delete (NOT WORKING)",
			"scope": [
				"keyword.operator.delete"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "HTML: doctype",
			"scope": [
				"meta.tag.metadata.doctype"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "HTML: attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "HTML: tag name",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "HTML: tag punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#898989"
			}
		},
		{
			"name": "CSS: property",
			"scope": [
				"meta.property-name"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "CSS: value",
			"scope": [
				"meta.property-value",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "CSS: important",
			"scope": [
				"keyword.other.important"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "CSS: value constants",
			"scope": [
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#e394dc"
			}
		},
		{
			"name": "CSS: value functions",
			"scope": [
				"support.function.shape.css"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "CSS: @media",
			"scope": [
				"keyword.control.at-rule"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "CSS: @media header",
			"scope": [
				"meta.at-rule",
				"keyword.operator.logical.and.media",
				"keyword.operator.logical.only.media"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "JS: variable",
			"scope": [
				"meta.definition.variable"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "JS: function name",
			"scope": [
				"entity.name.function.js"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
	]
}
