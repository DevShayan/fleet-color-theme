{
	"name": "Fleet Color Theme",
	"colors": {
		"editor.background": "#181818",
		"editor.foreground": "#d1d1d1",
		"focusBorder": "#0273eb",
		"editor.lineHighlightBackground": "#292929",
		"editor.lineHighlightBorder": "#00000000",
		"editorLineNumber.foreground": "#5d5d5d",
		"editorLineNumber.activeForeground": "#c2c2c2",
		"editorLineNumber.dimmedForeground": "#5d5d5d",
		"editorIndentGuide.background": "#00000000",
		"editorIndentGuide.activeBackground": "#00000000",
		"editorBracketMatch.background": "#163764",
		"editorBracketMatch.border": "#00000000",
		"editorBracketHighlight.foreground1": "#d1d1d1",
		"editorBracketHighlight.foreground2": "#d1d1d1",
		"editorBracketHighlight.foreground3": "#d1d1d1",
		"editorBracketHighlight.foreground4": "#d1d1d1",
		"editorBracketHighlight.foreground5": "#d1d1d1",
		"editorBracketHighlight.foreground6": "#d1d1d1",
		"editor.selectionBackground": "#163764",
		"editor.inactiveSelectionBackground": "#163764",
		"editorGroupHeader.tabsBackground": "#292929",
		"editor.foldBackground": "#00000000",
		"editorGutter.foldingControlForeground": "#767676",
		"activityBar.background": "#292929",
		"input.background": "#292929",
		"list.inactiveSelectionBackground": "#383838",
		"list.hoverBackground": "#2d2d2d",
		"list.focusAndSelectionOutline": "#00000000",
		"quickInput.background": "#292929",
		"quickInputList.focusBackground": "#194176",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#181818",
		"sideBar.border": "#383838",
		"statusBar.background": "#181818",
		"statusBar.noFolderBackground": "#181818",
		"statusBar.debuggingBackground": "#181818",
		"statusBar.border": "#383838",
		"statusBar.foreground": "#767676",
		"tab.inactiveBackground": "#292929",
		"tab.activeBackground": "#181818",
		"tab.activeBorderTop": "#d1d1d1",
		"tab.unfocusedActiveBorderTop": "#181818",
		"terminal.ansiBrightGreen":"#82d2ce",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#898989"
			}
		},
		{
			"name": "Keyword and Storage",
			"scope": [
				"keyword",
				"keyword.operator.instanceof",
				"variable.language",
				"storage"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#e394dc"
			}
		},
		{
			"name": "Escape Sequence",
			"scope": [
				"constant.character"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "Variable inside class",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "Function declaration",
			"scope": [
				"entity.name.function.definition",
				"entity.name.function.call",
				"meta.function.python"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#efefef"
			}
		},
		{
			"name": "C++: directives",
			"scope": [
				"keyword.control.directive",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "C++: bools",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "C++: character",
			"scope": [
				"string.quoted.single.cpp"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "C++: namespace name",
			"scope": [
				"entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "C++: Function call",
			"scope": [
				"entity.name.function.call"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#efefef"
			}
		},
		{
			"name": "C++: constructor",
			"scope": [
				"entity.name.function.definition.special.constructor"
			],
			"settings": {
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "C++: class and struct name",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "C++: NULL",
			"scope": [
				"constant.language.NULL"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "C++: defaults",
			"scope": [
				"punctuation.separator",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#d1d1d1"
			}
		},
		{
			"name": "C++: delete (NOT WORKING)",
			"scope": [
				"keyword.operator.delete"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "HTML: doctype",
			"scope": [
				"meta.tag.metadata.doctype"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "HTML: attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "HTML: tag name",
			"scope": [
				"meta.tag.metadata",
				"meta.tag.structure"
			],
			"settings": {
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "HTML: tag punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#898989"
			}
		},
		{
			"name": "XML: preprocessor",
			"scope": [
				"meta.tag.preprocessor.xml"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "XML: tag name",
			"scope": [
				"entity.name.tag.localname"
			],
			"settings": {
				"foreground": "#87c3ff"
			}
		},
		{
			"name": "CSS: tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "CSS: property",
			"scope": [
				"meta.property-name"
			],
			"settings": {
				"foreground": "#82d2ce"
			}
		},
		{
			"name": "CSS: value",
			"scope": [
				"meta.property-value",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "CSS: important",
			"scope": [
				"keyword.other.important"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "CSS: constants",
			"scope": [
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#e394dc"
			}
		},
		{
			"name": "CSS: functions",
			"scope": [
				"meta.function.shape.css",
				"meta.function.misc.css"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "CSS: @media",
			"scope": [
				"keyword.control.at-rule"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "CSS: @media header",
			"scope": [
				"meta.at-rule",
				"keyword.operator.logical.and.media",
				"keyword.operator.logical.only.media"
			],
			"settings": {
				"foreground": "#af9cff"
			}
		},
		{
			"name": "CSS: defaults",
			"scope": [
				"entity.name.tag.wildcard",
			],
			"settings": {
				"foreground": "#d1d1d1"
			}
		},
		{
			"name": "JAVA: Annotation",
			"scope": [
				"storage.type.annotation",
				"punctuation.definition.annotation"
			],
			"settings": {
				"foreground": "#a8cc7c"
			}
		},
		{
			"name": "JAVA: method name in call",
			"scope": [
				"meta.method-call.java"
			],
			"settings": {
				"foreground": "#ebc88d"
			}
		},
		{
			"name": "JAVA: method name in declaration",
			"scope": [
				"meta.method.identifier.java"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "JAVA: non-primitive data types",
			"scope": [
				"storage.type.java"
			],
			"settings": {
				"foreground": "#add1de"
			}
		},
		{
			"name": "JAVA: defaults",
			"scope": [
